resource "google_netapp_volume" "<%= ctx[:primary_resource_id] %>" {
  location = "us-central1"
  volume_id = "<%= ctx[:vars]['volume_name'] %>"
  capacity_gib = "100"
  share_name = "testshare"
  storage_pool = "testpool"
  protocols = ["NFSV3"]
  smb_settings = []
  unix_permissions = ""
  labels = {}
  description = ""
  snap_reserve = 0
  snapshot_directory = false
  security_style = "UNIX"
  kerberos_enabled = false
  restricted_actions = ""

}
